<?xml version="1.0"?>

<config>
    <modules>
        <Ccc_Test>
            <version>1.6.0.0.2</version>
        </Ccc_Test>
    </modules>
    <frontend>
        <routers>
            <test>
                <use>standard</use>
                <args>
                    <module>Ccc_Test</module>
                    <frontName>ccc</frontName>
                </args>
            </test>
        </routers>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <ccc_test before="Ccc_Banner_Adminhtml">Ccc_Test_Adminhtml</ccc_test>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <global>
        <models>
            <ccc_test>
                <class>Ccc_Test_Model</class>
                <resourceModel>test_resource</resourceModel>
            </ccc_test>
        </models>
    </global>
</config>